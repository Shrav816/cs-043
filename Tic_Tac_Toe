def helloWorld():
  print('Hello World!')
  print('I just used PyCharm to clone this, I have no idea if it works.')
  print('Okay, so I just "pulled" the code, and now I can see both yours and mine at the same time.')
  print('Can you update your function to say something if you see this code?')

def funkyfunction():
  print('Hopefully this works.')
  print("I added this from PyCharm")
  
helloWorld()
funkyfunction()